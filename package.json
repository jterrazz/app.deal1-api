{
  "name": "deal41-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@gitlab.com:jterrazz/deal41-api.git",
  "author": "Jean-Baptiste Terrazzoni <jterrazz@protonmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register build/server.js",
    "dev": "nodemon",
    "build": "tsc",
    "heroku-postbuild": "rm -rf ./build && yarn migration.run && tsc",
    "migration.generate": "ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "migration.create": "ts-node ./node_modules/.bin/typeorm migration:create -n",
    "migration.run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migration.revert": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "lint": "eslint --fix '**/*.{ts,js}'"
  },
  "dependencies": {
    "@types/node": "^11.12.1",
    "apollo-server-koa": "^2.4.8",
    "graphql": "^14.2.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.11",
    "pg": "^7.9.0",
    "pino": "^5.12.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "typeorm": "^0.2.16",
    "typescript": "^3.3.4000"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "nodemon": "^1.18.10",
    "pino-pretty": "^2.5.0",
    "prettier": "^1.16.4"
  }
}
